{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && ./scripts/setup.sh",
    "build": "rm -rf vendor/server/Server/mods/*.jar && ./gradlew jar && cp app/build/libs/* vendor/server/Server/mods/ && tmux send-keys -t hytale \"plugin reload Bellian:Test\" C-m && echo 'Build complete!'",
    "dev": "onchange './app/src/**/*' -- npm run build",
    "dev:server": "cd vendor/server/Server && java -Xmx4G -Xms4G -jar HytaleServer.jar --assets ../Assets.zip",
    "dev:server:tmux": "tmux new -s hytale 'cd vendor/server/Server && java -Xmx4G -Xms4G -jar HytaleServer.jar --assets ../Assets.zip'",
    "hytale-downloader": "cd vendor/hytale-downloader && ./hytale-downloader-linux-amd64 -download-path ../server.zip",
    "hytale-downloader:version": "cd vendor/hytale-downloader && ./hytale-downloader-linux-amd64 -download-path ../server.zip -print-version",
    "hytale-downloader:update": "cd vendor/hytale-downloader && ./hytale-downloader-linux-amd64 -check-update"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "concurrently": "^9.2.1",
    "onchange": "^7.1.0"
  }
}
